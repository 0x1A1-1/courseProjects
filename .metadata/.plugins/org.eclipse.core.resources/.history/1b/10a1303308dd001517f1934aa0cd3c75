class Solution(object):  
    def twoSum(self, nums, target):  
        """ 
        :type nums: List[int] 
        :type target: int 
        :rtype: List[int] 
        """  
        snums = set(nums)  
        for i, v in enumerate(nums):  
            if target - v in snums and target - v in nums[i+1:]:  
                return [i, nums[i+1:].index(target - v)+i+1]  