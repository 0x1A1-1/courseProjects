class Point(object):
     def __init__(self, a=0, b=0):
         self.x = a
         self.y = b
         
class Solution(object):
    def maxPoints(self, points):
        """
        :type points: List[Point]
        :rtype: int
        """
        max=0
        if len(points) <= 1:
            return max
        for i in range(len(points)):
            slope = []
            vert = 0;
            for j in range(i, len(points)):
                if points[i].x==points[j].x:
                    vert+=1
                    continue
                slope.append((points[i].y-points[j].y)/(points[i].x-points[j].x))
            tempmax = max( slope.count(x) for x in slope ) 
            if tempmax>max:
                max=tempmax
        return max+1
    ls = []
    ls.append( Point(1, 2) )
    ls.append( Point( 1, 3) )
    ls.append( Point( 1, 4) )
    ls.append( Point( 1, 5) )
    maxPoints(object, ls)