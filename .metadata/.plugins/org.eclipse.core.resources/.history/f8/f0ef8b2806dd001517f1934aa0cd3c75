class Solution(object):
   
    def lengthOfLongestSubstring(self, s):
        """
        :type s: str
        :rtype: int
        """
        maxLen = 0 
        curr = ""
        for j in range (0, len(s)):
            if s[j] in curr:
                if (len(curr)>maxLen):
                    maxLen=len(curr)
                curr = curr[curr.index(s[j])+1:]+s[j]
                
            elif (j==len(s)-1):
                curr+=s[j]
                if (len(curr)>maxLen):
                    maxLen=len(curr)
            else:
                curr+=s[j]
        return maxLen
    
    print(lengthOfLongestSubstring(object,"abcdabcdefax"))
    