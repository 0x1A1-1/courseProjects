class Solution(object):
    def threeSum(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        dict = {}
        ans = []
        if nums.count(0)>=3:
            ans.append([0,0,0])
        for i in nums:
            if i not in dict:
                dict[i]=min(2,nums.count(i))
        
        newNums = []
        for i in dict:
            for n in range(dict[i]):
                newNums.append(i)
        print(newNums)   
    threeSum(object,[-2,-1,0,1,2]) 