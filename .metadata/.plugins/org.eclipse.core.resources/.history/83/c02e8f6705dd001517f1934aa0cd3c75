class Solution(object):
    def lengthOfLongestSubstring(self, s):
        """
        :type s: str
        :rtype: int
        """
        max = 0 
        curr = ""
        for j in range (0, len(s)):
            if s[j] in curr:
                if (len(curr)>max):
                    max=len(curr)
                curr = curr[curr.index(s[j]):]+s[j]
            else:
                curr+=s[j]
            
        return max